{
    "name": "SessionHistory",
    "version": "0.1",
    "manifest_version": 2,
    "description": "This extension can allow you easily return closed tabs",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "sandbox": {
        "pages": ["render.html"]
    },
    "options_page": "options.html",

    "background": {
        "page" : "background.html"},

    "permissions": [
        "tabs",
        "<all_urls>"
    ],
    "icons": {
        "128": "icon.png"
    }
}